+ (NSURLRequest *)fetchCommentAdd:(NSDictionary *)parameters
                          success:(void (^)(id result))success
                          failure:(void (^)(NSError *error))failure;
{
    NSURLRequest *request = [HNCFRequestManager post:[HNCF_APP_URL stringByAppendingPathComponent:USER_COMENTADD]
                                          parameters:parameters
                                             success:^(id data) {
                                                 success(data);
                                             }
                                             failure:^(NSError *error) {
                                                 failure(error);
                                             }];
    return request;

}
